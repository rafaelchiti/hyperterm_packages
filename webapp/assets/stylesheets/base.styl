@import '~webapp/assets/stylesheets/normalize'
@import '~webapp/assets/stylesheets/glyphicons'

body {
  font-family: 'Helvetica', 'Lucida Grande', 'sans-serif';
  background-color: black;
  color: white;
  overflow-y: scroll;
}

html {
  box-sizing: border-box;
}

*, *:before, *:after {
  box-sizing: inherit;
}

a {
  color: black;
  text-decoration: none;
}
